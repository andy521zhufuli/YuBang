<?xml version="1.0" encoding="utf-8"?>
<!-- 电子电路  整个-->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#ffffffff"
    >
    <!-- 保养维护  更多-->
    <RelativeLayout
        android:id="@+id/iwant_get_cash_account_weixin_account_layout2"
        android:paddingLeft="23.0dip"
        android:clickable="true"
        android:layout_width="fill_parent"
        android:layout_height="43.0dip"
        android:background="#ffffffff"
        >
        <TextView
            android:id="@+id/main_product_name_2"
            android:textSize="15.0sp"
            android:textColor="@color/holo_green_dark"
            android:paddingLeft="15.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="电子电路"
            android:layout_centerVertical="true" />

        <TextView
            android:textSize="15.0sp"
            android:textColor="@color/baoyangweihu"
            android:id="@+id/shoppingmall_more_02"
            android:paddingLeft="15.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="更多   "
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:drawableRight="@mipmap/android_list_idex"
            android:layout_centerVertical="true" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        style="@style/line_vertical"
        />
    <!-- 保养维护 一行显示两个-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/main_product2_01"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@mipmap/shoppingmall_dianzidianlu_01"
            android:gravity="center"
            android:scaleType="fitXY"
            />

        <View android:layout_width="1.0px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            style="@style/line_vertical" />
        <ImageView
            android:id="@+id/main_product2_02"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@mipmap/shoppingmall_dianzidianlu_02"
            android:scaleType="fitXY"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        style="@style/line_vertical"
        />
    <!-- 保养维护 一行显示四个-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:orientation="horizontal"
        >
        <ImageView
            android:id="@+id/main_product2_03"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@mipmap/shoppingmall_baoyang_03"
            android:gravity="center"
            />
        <View
            android:layout_width="1.0px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            style="@style/line_vertical" />
        <ImageView
            android:id="@+id/main_product2_04"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@mipmap/shoppingmall_baoyang_04"
            android:gravity="center"
            />
        <View
            android:layout_width="1.0px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            style="@style/line_vertical" />
        <ImageView
            android:id="@+id/main_product2_05"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@mipmap/shoppingmall_baoyang_05"
            android:gravity="center"
            />
        <View
            android:layout_width="1.0px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            style="@style/line_vertical" />
        <ImageView
            android:id="@+id/main_product2_06"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@mipmap/shoppingmall_baoyang_06"
            android:gravity="center"
            />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        style="@style/line_vertical"
        />
    <ImageView
        android:id="@+id/main_product_ad_2"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:src="@mipmap/shoppingmall_ad_02"
        android:scaleType="fitXY"
        />
</LinearLayout>