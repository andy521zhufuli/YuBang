	<package name="GOODS" extends="json-default">

        <!-- 商品查询 -->
		<action name="LoadGoodsList" class="LoadGoodsListAction" method="doAction">
		<result name="0">/jsp/LoadGoodsList.jsp</result>
		<!--result name="1">/json/error.jsp</result>
		<result name="2">/json/sucess.jsp</result>
		<result name="3">/json/null.jsp</result>
		<result name="4">/json/ArgsError.jsp</result>
		<result name="5">/json/error.jsp</result-->
		</action>
		
		 <!-- 商品详情查询 -->
		<action name="LoadGoodsDetail" class="LoadGoodsDetailAction" method="doAction">
		<result name="0">/jsp/gooddetail.jsp</result>
		<result name="1">/json/error.jsp</result>
		<result name="2">/json/sucess.jsp</result>
		<result name="3">/json/null.jsp</result>
		<result name="4">/json/ArgsError.jsp</result>
