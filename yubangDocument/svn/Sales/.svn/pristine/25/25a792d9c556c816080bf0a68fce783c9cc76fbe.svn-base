package com.common.dc;

import java.util.List;
import java.util.Map;

public class DCconfig {
	
	//刷新时间
	private long refreshTime;
	
	//表名
	private String tableName;
	
	//查询主键名字
	private List<String> pkList;
	
	//需要加载的列
	private List<String> columnList;
	
	//过滤条件
	private List<String> condition;
	
	//刷新监听器
	private List<DCListenter> listenters;
	
	private DCDao dcDao;

	public long getRefreshTime() {
		return refreshTime;
	}

	public void setRefreshTime(long refreshTime) {
		this.refreshTime = refreshTime;
	}

	public String getTableName() {
		return tableName;
	}

	public void setTableName(String tableName) {
		this.tableName = tableName;
	}

	public List<String> getPkList() {
		return pkList;
	}

	public void setPkList(List<String> pkList) {
		this.pkList = pkList;
	}

	public List<String> getColumnList() {
		return columnList;
	}

	public void setColumnList(List<String> columnList) {
		this.columnList = columnList;
	}

	public List<String> getCondition() {
		return condition;
	}

	public void setCondition(List<String> condition) {
		this.condition = condition;
	}

	public List<DCListenter> getListenters() {
		return listenters;
	}

	public void setListenters(List<DCListenter> listenters) {
		this.listenters = listenters;
	}

	public DCDao getDcDao() {
		return dcDao;
	}

	public void setDcDao(DCDao dcDao) {
		this.dcDao = dcDao;
	}
	
	

}
